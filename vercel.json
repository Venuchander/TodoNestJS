{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "prisma/schema.prisma",
            "node_modules/.prisma/client/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/main.js"
      }
    ],
    "env": {
      "DATABASE_URL": "@database_url"
    }
  }
  